<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.softwareen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home_screen"></activity>
        <activity android:name=".temperary_correct_sign_up_validation" />
        <activity android:name=".temperaryErrorPage" />
        <activity android:name=".welcome_screen_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".welcome_screen_signup"
            android:parentActivityName=".welcome_screen_login" />
        <activity
            android:name=".home_profile"
            android:parentActivityName=".home_screen" />
        <activity
            android:name=".add_substance_list"
            android:parentActivityName=".home_screen" />
        <activity
            android:name=".add_create_substance"
            android:parentActivityName=".add_substance_list" />
    </application>

</manifest>